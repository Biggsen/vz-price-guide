{
  "name": "vz-price-guide",
  "version": "1.0.0",
  "description": "VZ's economic price guide for Minecraft 1.16",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "sass --style=compressed resources/sass/main.scss css/main.css",
    "build:html": "npx @11ty/eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "sass --style=compressed --watch resources/sass/main.scss css/main.css",
    "watch:html": "npx @11ty/eleventy --serve",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Biggsen/vz-price-guide.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Biggsen/vz-price-guide/issues"
  },
  "homepage": "https://github.com/Biggsen/vz-price-guide#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sass": "^1.32.8"
  }
}
